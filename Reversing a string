#include<stdio.h>
int main()
{

  char A[] = "Hello World";
  printf("Original string : %s\n", A);

  //reversing a string 
  printf("Reversed String : %s", strrev(A));

  char B[] = "Javaprogramming"
  print("Original string : %s\n",B);

  //reversing a string 
  print("Reversed string : %s", strrev(B));

  return 0;
  }
  
